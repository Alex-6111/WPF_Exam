<Window x:Class="WPF_Exam.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Exam"
        mc:Ignorable="d"
        Title="Rick And Morty" Height="950" Width="1100" Background="#4b636e" WindowStartupLocation="CenterScreen">
    <Grid>
        <Frame x:Name="Main" Content="Frame" NavigationUIVisibility="Hidden" HorizontalAlignment="Left" Height="300" Margin="144,55,0,0" VerticalAlignment="Top" Width="500"/>
        <Button x:Name="species_button" Content="Species" Background="#a7c0cd" Foreground="Black" HorizontalAlignment="Left" Margin="23,100,0,0" VerticalAlignment="Top" Width="100" Click="Species_button_Click" />
        <Button x:Name="charsPage_button" Content="Characters" Background="#a7c0cd" Foreground="Black" HorizontalAlignment="Left" Margin="23,55,0,0" VerticalAlignment="Top" Width="100" Click="CharsPage_button_Click"/>
    </Grid>
</Window>
